/* Compilation flags */
#define GFS_COMPILATION_FLAGS "@CMAKE_C_FLAGS@"

/* Major version */
#define GFS_MAJOR_VERSION @GFS_MAJOR_VERSION@

/* Micro version */
#define GFS_MICRO_VERSION @GFS_MICRO_VERSION@

/* Minor version */
#define GFS_MINOR_VERSION @GFS_MINOR_VERSION@

/* Version */
#cmakedefine GFS_VERSION "@GFS_VERSION@"

/* Define to 1 if you have the <dlfcn.h> header file. */
#cmakedefine HAVE_DLFCN_H 1

/* Define to 1 if you have the <fenv.h> header file. */
#cmakedefine HAVE_FENV_H 1

/* Define to 1 if you have the `funopen' function. */
#cmakedefine HAVE_FUNOPEN 1

/* Define to 1 if you have the <getopt.h> header file. */
#cmakedefine HAVE_GETOPT_H 1

/* Define to 1 if you have the `getopt_long' function. */
#cmakedefine HAVE_GETOPT_LONG 1

/* Conan ensures we have `g_mkdtemp' function. */
#define HAVE_G_MKDTEMP 1

/* Define to 1 if you have the <inttypes.h> header file. */
#cmakedefine HAVE_INTTYPES_H 1

/* Conan ensures we have m4 available. */
#define HAVE_M4 1

/* Define to 1 if you have the <memory.h> header file. */
#cmakedefine HAVE_MEMORY_H 1

/* Define to 1 if you have MPI */
#cmakedefine HAVE_MPI 1

/* Define to 1 if you have the `open_memstream' function. */
#cmakedefine HAVE_OPEN_MEMSTREAM 1

/* Define to 1 if GModule are supported and if you have pkg-config. */
#cmakedefine HAVE_PKG_CONFIG 1

/* Define to 1 if you have the <stdint.h> header file. */
#cmakedefine HAVE_STDINT_H 1

/* Define to 1 if you have the <stdlib.h> header file. */
#cmakedefine HAVE_STDLIB_H 1

/* Define to 1 if you have the <strings.h> header file. */
#cmakedefine HAVE_STRINGS_H 1

/* Define to 1 if you have the <string.h> header file. */
#cmakedefine HAVE_STRING_H 1

/* Define to 1 if you have the <sys/stat.h> header file. */
#cmakedefine HAVE_SYS_STAT_H 1

/* Define to 1 if you have the <sys/types.h> header file. */
#cmakedefine HAVE_SYS_TYPES_H 1

/* Define to 1 if you have the <unistd.h> header file. */
#cmakedefine HAVE_UNISTD_H 1

/* Options for m4 */
#define M4_OPTIONS "-P"

/* Define to 1 if you have the ANSI C header files. */
#define STDC_HEADERS 1
